{
  "name": "atlas",
  "version": "0.1.0",
  "description": "Intelligent web crawler for comprehensive site mapping",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:manual": "npm run build && esbuild scripts/manual-test.ts --bundle --platform=node --format=esm --outfile=dist/manual-test.mjs --packages=external --keep-names=false && node dist/manual-test.mjs",
    "visualize": "npx tsx scripts/visualize.ts",
    "clean": "rimraf dist output",
    "clean:dist": "rimraf dist",
    "clean:output": "rimraf output",
    "prepare": "husky"
  },
  "keywords": [
    "crawler",
    "web-scraper",
    "sitemap",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "rimraf": "6.1.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "cheerio": "1.1.2",
    "crawlee": "3.15.3",
    "fast-xml-parser": "5.3.3",
    "puppeteer": "24.33.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "zod": "4.1.13"
  }
}
